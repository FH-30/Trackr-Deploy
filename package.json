{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend of Trackr",
  "main": "server.js",
  "author": "Francis Hodianto",
  "license": "MIT",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "build": "cd client && yarn build",
    "install-client": "cd client && yarn",
    "heroku-postbuild": "yarn run install-client && yarn run build",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-query-parser": "^1.0.2",
    "express-rate-limit": "^5.1.3",
    "hashmap": "^2.4.0",
    "helmet": "^3.23.3",
    "is-empty": "^1.2.0",
    "js-priority-queue": "^0.1.5",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.27.0",
    "mongoose": "^5.9.16",
    "node-schedule": "^1.3.2",
    "node-scheduler": "^1.0.0",
    "nodemailer": "^6.4.8",
    "optimized-quicksort": "^1.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "priorityqueue": "^1.0.0",
    "url": "^0.11.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
